<models>
    <model file="powersuits:item/armor/mps_helm.obj" textures="powersuits:models/diffuse.png">
        <binding slot="head" target="Head">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="helm_main" name="Helmet"/>
            <part defaultcolor="114EFF" defaultglow="true" polygroup="visor" name="Visor"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="helm_tubes" name="Tubes"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="helm_tube_entry2" name="Right Tube Entry"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="helm_tube_entry1" name="Left Tube Entry"/>
        </binding>
    </model>
    <model file="powersuits:item/armor/mps_chest.obj" textures="powersuits:textures/models/diffuse.png">
        <binding slot="chest" target="Body">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="backpack" name="Backpack"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="polySurface36" name="Accessory"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="belt" name="Belt"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="chest_padding" name="Chest Padding"/>
            <part defaultcolor="114EFF" defaultglow="true" polygroup="crystal_belt" name="Belt Crystal"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="chest_main" name="Chest Plating"/>
        </binding>
    </model>
    <model file="powersuits:item/armor/jetpack.obj" textures="powersuits:textures/models/jetpack.png">
        <binding slot="chest" target="Body">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="default" name="Main"/>
            <part defaultcolor="114EFF" defaultglow="false" polygroup="jetpack5" name="Secondary"/>
            <part defaultcolor="114EFF" defaultglow="true" polygroup="jetpack_glow" name="Lights"/>
        </binding>
    </model>
    <model file="powersuits:item/armor/mps_arms.obj" textures="powersuits:textures/models/diffuse.png">
        <binding slot="chest" target="RightArm">
            <part defaultcolor="114EFF" defaultglow="true" polygroup="crystal_shoulder_2"
                  name="Right Shoulder Crystal"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="arms3" name="Right Arm"/>
        </binding>
        <binding slot="chest" target="LeftArm">
            <part defaultcolor="114EFF" defaultglow="true" polygroup="crystal_shoulder_1" name="Left Shoulder Crystal"/>
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="arms2" name="Left Arm"/>
        </binding>
    </model>
    <model file="powersuits:item/armor/mps_pantaloons.obj" textures="powersuits:textures/models/diffuse.png">
        <binding slot="legs" target="LeftLeg">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="leg2" name="Left Leg"/>
        </binding>
        <binding slot="legs" target="RightLeg">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="leg1" name="Right Leg"/>
        </binding>
    </model>
    <model file="powersuits:item/armor/mps_boots.obj" textures="powersuits:textures/models/diffuse.png">
        <binding slot="feet" target="RightLeg">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="boots1" name="Right Boot"/>
        </binding>
        <binding slot="feet" target="LeftLeg">
            <part defaultcolor="FFFFFF" defaultglow="false" polygroup="boots2" name="Left Boot"/>
        </binding>
    </model>
</models>